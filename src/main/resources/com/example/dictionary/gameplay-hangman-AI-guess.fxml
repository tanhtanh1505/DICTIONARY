<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.dictionary.GameplayHangmanAIGuess">
   <children>
      <Label fx:id="secretWord" layoutX="43.0" layoutY="37.0" text="Hello">
         <font>
            <Font name="Times New Roman Bold" size="24.0" />
         </font>
      </Label>
      <Label fx:id="guess" layoutX="43.0" layoutY="106.0" text="Labell">
         <font>
            <Font name="Times New Roman Bold" size="24.0" />
         </font>
      </Label>
      <Button layoutX="36.0" layoutY="304.0" mnemonicParsing="false" onAction="#agree" text="Yes" />
      <Button layoutX="109.0" layoutY="304.0" mnemonicParsing="false" onAction="#disagree" text="No" />
      <TextField fx:id="mask" layoutX="36.0" layoutY="243.0" />
      <Label fx:id="remainingG" layoutX="396.0" layoutY="41.0" text="Remaining Guess:">
         <font>
            <Font name="Times New Roman Bold" size="18.0" />
         </font>
      </Label>
      <Label fx:id="listWordGuessed" layoutX="40.0" layoutY="179.0" text="Label">
         <font>
            <Font name="System Bold" size="18.0" />
         </font></Label>
      <Button layoutX="493.0" layoutY="335.0" mnemonicParsing="false" onAction="#exit" text="Exit" />
   </children>
</AnchorPane>
