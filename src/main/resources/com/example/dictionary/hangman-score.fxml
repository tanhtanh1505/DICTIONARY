<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.paint.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.dictionary.HangManViewScore">
   <children>
      <ImageView fitHeight="414.0" fitWidth="660.0" layoutY="1.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../image/rank.png" />
         </image>
      </ImageView>
      <Label layoutX="116.0" layoutY="24.0" text="Rank">
         <font>
            <Font name="Times New Roman Bold" size="36.0" />
         </font>
         <textFill>
            <LinearGradient endX="1.0" endY="1.0">
               <stops>
                  <Stop color="#59a16d" />
                  <Stop color="#e8ad24" offset="1.0" />
               </stops>
            </LinearGradient>
         </textFill>
      </Label>
      <ListView fx:id="listRankShow" layoutX="116.0" layoutY="94.0" prefHeight="228.0" prefWidth="392.0" style="-fx-background-color: rgba(255, 181, 232, 0.2);" />
      <Button layoutX="289.0" layoutY="346.0" mnemonicParsing="false" onAction="#exit" style="-fx-background-color: rgba(85, 181, 232, 0.8);" text="Exit">
         <cursor>
            <Cursor fx:constant="CLOSED_HAND" />
         </cursor>
         <font>
            <Font name="System Bold Italic" size="15.0" />
         </font></Button>
   </children>
</AnchorPane>
